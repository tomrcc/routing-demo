{
  "routes": [
    {
      "from": "/blog/all-the-bells-and-whistles*",
      "to": "/blog/email-delivery-tips/",
      "status": 301
    }
  ],
  "headers": [
    {
      "match": "*",
      "headers": [
        {
          "name": "Access-Control-Allow-Origin",
          "value": "https://example.com"
        },
        {
          "name": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "name": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
